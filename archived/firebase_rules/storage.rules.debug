rules_version = '2';

// Regras ULTRA PERMISSIVAS apenas para DEBUGGING
// NÃO usar em produção!

service firebase.storage {
  match /b/{bucket}/o {
    
    // Permitir tudo para usuários autenticados (apenas para debug)
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}