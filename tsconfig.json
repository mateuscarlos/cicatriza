{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "tamagui": ["./node_modules/tamagui"],
      "tamagui/*": ["./node_modules/tamagui/*"],
      "tamagui.config": ["./tamagui.config"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
